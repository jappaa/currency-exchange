describe('currency popular action', function () {
  it('creates LOAD_CURRENCY_POPULAR_PAGE action', function () {
    var login = 0;
    var requiredFields = [];
    var expectedAction = {
      type: ActionTypes.LOAD_CURRENCY_POPULAR_PAGE,
