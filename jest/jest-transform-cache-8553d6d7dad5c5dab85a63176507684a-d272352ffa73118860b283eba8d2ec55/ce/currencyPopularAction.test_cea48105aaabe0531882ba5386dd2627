      requiredFields: requiredFields };

    expect(ActionTypes.loadCurrencyPopularPage(login, requiredFields)).toEqual(
    expectedAction);

  });

  it('creates UPDATE_CURRENCY_POPULAR action', function () {
    var index = 0;
    var data = [];
